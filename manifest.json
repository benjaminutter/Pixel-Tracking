{
    "manifest_version": 3,
    "name": "Tracking Pixel Blocker",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": [
      "scripting",
      "webRequest"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "*://*/"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  